datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Ticket {
  id           Int      @id @default(autoincrement())
  title        String
  description  String
  status       String   @default("open")
  emailThreadId String  @default("")
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
}
